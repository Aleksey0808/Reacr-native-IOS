{
  "expo": {
    "name": "ma-app",
    "slug": "ma-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "jsEngine": "jsc"
     
    },
    "android": {
      "jsEngine": "jsc",
     
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.aleksey0808.maapp"
    },
    "extra": {
      "eas": {
        "projectId": "012d9038-ab28-484b-acff-0c3b7d3cf365"
      }
    }
  }
}
